{
  "name": "figma-plugin-vue-vite",
  "type": "module",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "scripts": {
    "dev": "pnpm watch",
    "build": "run-s build:*",
    "dev:ui": "vite -c ./vite.config.ui.ts",
    "watch": "run-p watch:*",
    "watch:ui": "pnpm build:ui --watch --mode=development",
    "watch:plugin": "pnpm build:plugin --watch --mode=development",
    "clean": "rimraf dist/*",
    "prebuild": "run-s clean",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "*",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.3.3",
    "unocss": "^0.60.3",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.11",
    "vite-plugin-generate-file": "^0.1.1",
    "vite-plugin-singlefile": "^2.0.1",
    "vue-tsc": "^2.0.19"
  }
}
